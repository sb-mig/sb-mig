{
  "name": "sb-mig",
  "version": "4.0.12-beta.1",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "license": "MIT",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "homepage": "https://github.com/sb-mig/sb-mig",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "rm -rf dist && tsc && chmod +x ./dist/index.js",
    "build:dev": "chokidar 'src/**/*.{js,ts,cjs,mjs}' -c 'yarn build'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn build && ./dist/index.js",
    "debug": "yarn build && ./dist/index.js sync components accordion accordion-item",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "dotenv": "^16.0.1",
    "glob": "^8.0.3",
    "meow": "^10.1.3",
    "ncp": "^2.0.0",
    "storyblok-js-client": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/glob": "^7.2.0",
    "@types/ncp": "^2.0.5",
    "@types/node": "^18.7.13",
    "chokidar-cli": "^3.0.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
